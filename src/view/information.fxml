<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.InformationView">
   <children>
      <ImageView fx:id="img2" layoutX="5.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/info2.png" />
         </image>
      </ImageView>
      <ImageView fx:id="img" layoutX="3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/info.png" />
         </image>
      </ImageView>
      <Button fx:id="b" layoutX="94.0" layoutY="469.0" mnemonicParsing="false" onAction="#info2" opacity="0.0" prefHeight="40.0" prefWidth="72.0" style="-fx-cursor: hand;">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="BLACK" offset="1.0" />
               </stops>
            </RadialGradient>
         </textFill>
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="backToMain" cancelButton="true" layoutX="642.0" layoutY="560.0" mnemonicParsing="false" onAction="#backToMain" opacity="0.0" prefHeight="47.0" prefWidth="256.0" style="-fx-cursor: hand;">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="BLACK" offset="1.0" />
               </stops>
            </RadialGradient>
         </textFill>
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="40.0" layoutY="514.0" mnemonicParsing="false" onAction="#back" opacity="0.0" prefHeight="37.0" prefWidth="72.0" style="-fx-cursor: hand;">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="BLACK" offset="1.0" />
               </stops>
            </RadialGradient>
         </textFill>
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
